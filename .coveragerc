[run]
source = .
omit = 
    */racing-venv/*
    */tests/*
    */test_*
    setup.py
    */migrations/*
    */config/*
    */__pycache__/*
    */site-packages/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstract